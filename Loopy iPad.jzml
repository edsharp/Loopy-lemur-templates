<SCRIPT name="reset()" script="slidersTouched = fill(1,0,numStores);

decl obj,tmp,i=0;

obj = getfirst(Controls);
while(obj)
{
	tmp=getobjectrect(obj);
	tmp[0]= i * 107;
	tmp[1]= -10;
	//tmp[2]= faderWidth;
	//tmp[3]= faderHeight;
	setattribute(obj,'rect',tmp);
	setexpression(obj,'i',i);
	ary[i] = obj;
	i++;
	obj=getnext(obj);
}" trigger_script="" trigger_type="4" trigger="1" osc_message="/Presets/Controls/reset" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
