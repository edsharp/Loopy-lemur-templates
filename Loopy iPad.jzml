<SCRIPT name="recordSelectedTrack()" script="if (isRecording()) {
	stopRecording();

	if (firstEmpty < maxTracks) {
		recordSelectedTrack();
	}
} else {
	if (!canRecord()) {
		setNextAction('');
		return;
	}

	if (Mute.x) {
		Mute.reset();
		toSelectedTracks('mute');
	}

	if (sumof(selectedTracks) == 1) {
		recordingTrack = firstof(selectedTracks);
	} else {
		recordingTrack = firstEmpty;
	}

	trackStates[recordingTrack] = 1;

	noteout(0,46+recordingTrack,127,1);
	noteout(0,46+recordingTrack,0,1);
}

setNextAction('');

updateTrackColors();" trigger_script="Record.x" trigger_type="0" trigger="3" osc_message="/recordSelectedTrack" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="resetSession()" script="show(ClearReally,0);
setattribute(ClearReally,'color',HSV(.083,1,1));
setattribute(ResetSession,'color',{RGB(1,1,1),HSV(.083,1,1)});
setattribute(ClearTrack,'color',{RGB(1,1,1),HSV(.083,1,1)});
show(ResetSessionReally,0);
setattribute(ResetSessionReally,'color',HSV(.083,1,1));

noteout(0,27,127,1);
noteout(0,27,0,1);
trackStates = fill(1,0,maxTracks);
trackVolumes = fill(1,0.5,maxTracks);
VFaders.reset();
trackPans = fill(1,0.5,maxTracks);
MultiPan.reset();
recordLength = 1;
BeatLeds.value = 0;
MIDIClock.continuesong = 0;
MIDIClock.last_continue = 0;
MIDIClock.last_start = 0;
MIDIClock.last_stop = 0;
MIDIClock.songpos = 0;
MIDIClock.startsong = 0;
MIDIClock.stopsong = 0;
MIDIClock.tick = 0;

recordingTrack=maxTracks;
clearSelection();
selectedTracks[0] = 1;
Presets.reset(); // Must be after clearSelection
updateTrackColors();
" trigger_script="" trigger_type="5" trigger="1" osc_message="/resetSession" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="putTracksInState(tracks, state)" script="if (state < 2) return; // Don't set to record or empty

decl i;
for (i=0;i<maxTracks;i++) {
	// Ignore some
	if (!tracks[i]) continue;
	// Ignore empty or correct
	if (!trackStates[i] || (trackStates[i] == state)) continue;

	if (trackStates[i] == 1) {
		// If recording use select track
		if (state == 3) {
			// We want play, so just toggle
			noteout(0,46+i,127,1); // Toggle record
			noteout(0,46+1,0,1); // Toggle record
			trackStates[i] = 3;
		} else {
			// We want stop, so mute
			setSelectedTrack(i);

			noteout(0,33,127,1); // Toggle record, mute, then select next track
			noteout(0,33,0,1); // Toggle record, mute, then select next track
			trackStates[i] = 2;
		}
	} else {
		// Otherwise just toggle
		muteTrack(i);
	}
}
updateTrackColors();" trigger_script="" trigger_type="4" trigger="1" osc_message="/putTracksInState" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
