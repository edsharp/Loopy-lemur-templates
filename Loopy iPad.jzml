	updateTrackColors();
} else {
	colorTracks();
}
" trigger_script="" trigger_type="4" trigger="1" osc_message="/Presets/Controls/sliderReleased" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="colorTracks()" script="decl s,t;
decl colors = fill(1,emptyPadColor,maxTracks);
decl tracks;

for (s=0;s<numStores;s++) {
	if (!slidersTouched[s]) continue;
	tracks = getSelection(s);
	for (t=0;t<maxTracks;t++) {
		if (tracks[t]) {
			colors[t] = touchColor;
		}
		setattribute(VFaders.ary[t],'color',colors[t]);
	}
}

setattribute(Tracks,'colors',colors);" trigger_script="" trigger_type="4" trigger="1" osc_message="/Presets/Controls/colorTracks" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
