<SCRIPT name="reset()" script="decl obj,tmp,i=0;

obj = getfirst(VFaders);
while(obj)
{
	tmp=getobjectrect(obj);
	tmp[0]= (faderWidth-5) * i;
	tmp[1]= 0;
	tmp[2]= faderWidth;
	tmp[3]= faderHeight;
	setattribute(obj,'rect',tmp);
	setexpression(obj,'x',trackVolumes[i]);
	setexpression(obj,'i',i);
	ary[i] = obj;
	i++;
	obj=getnext(obj);
}" trigger_script="" trigger_type="4" trigger="1" osc_message="/VFaders/reset" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="sliderReleased()" script="decl t;
decl colors = fill(1,emptyPadColor,maxTracks);
decl stillPressed=0;

for (t=0;t<maxTracks;t++) {
	if (getexpression(ary[t],'z')) {
		colors[t] = touchColor;
		stillPressed=1;
	}
	setattribute(ary[t],'color',colors[t]);
}

if (!stillPressed) {
	updateTrackColors();
}" trigger_script="" trigger_type="4" trigger="1" osc_message="/VFaders/sliderReleased" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
