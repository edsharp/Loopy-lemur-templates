<SCRIPT name="toSelectedTracks(action)" script="decl i;

for (i=0;i<maxTracks;i++) {
	if (!selectedTracks[i]) continue;

	if (action == 'clear') {
		setSelectedTrack(i);
		clearSelectedTrack();
	} else if (action == 'mute') {
		muteTrack(i);
	}
}

setNextAction('');
updateTrackColors();" trigger_script="" trigger_type="4" trigger="1" osc_message="/toSelectedTracks" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
