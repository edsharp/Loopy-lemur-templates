<SCRIPT name="muteTrack(i)" script="if (trackStates[i] == 0) {
	return;
}

noteout(0,34+i,127,1);
noteout(0,34+i,0,1);

if (trackStates[i] > 1) {
	if (trackStates[i] == 3) {
		trackStates[i] = 2;
	} else {
		trackStates[i] = 3;
	}
} else {
	trackStates[i] = 3;
	recordingTrack = maxTracks;
}" trigger_script="" trigger_type="4" trigger="1" osc_message="/muteTrack" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="_updTrks(tracks, action)" script="decl alt = 1;
decl labels = fill(1,'',maxTracks);

decl colors = fill(1,emptyPadColor,maxTracks);
firstEmpty = maxTracks;

decl i;
for(i=0; i<maxTracks; i++) {
	if (trackStates[i] == 3) { // Playing content
		labels[i] = 'Playing';

		if (tracks[i]) {
			colors[i] = colPlayS;
		} else {
			colors[i] = colPlay;
		}

		if (tracks[i]) {
			if ((action=='stop') || (action=='mute') || (action=='toggleAndRecord') || (action=='togglePlayRec') || (action=='toggleStopRec')) {
				labels[i] = 'Will stop';
				if (alt) colors[i] = colStopS;
			}

			if ((action=='record') && (sumof(tracks)==1)) {
				labels[i] = 'Will overdub';
				if (alt) colors[i] = colRecS;
			}

			if (action=='clearTracks') {
				labels[i] = 'Will clear';
				if (alt) colors[i] = colEmptyS;
			}
		}


	} else if(trackStates[i] == 2) { // Stopped content
		labels[i] = 'Stopped';

		if (tracks[i]) {
			colors[i] = colStopS;
		} else {
			colors[i] = colStop;
		}

		if (tracks[i]) {
			if ((action=='play') || (action=='mute') || (action=='toggleAndRecord') || (action=='togglePlayRec') || (action=='toggleStopRec')) {
				labels[i] = 'Will play';
				if (alt) colors[i] = colPlayS;
			}

			if ((action=='record') && (sumof(tracks)==1)) {
				labels[i] = 'Will overdub';
				if (alt) colors[i] = colRecS;
			}

			if (action=='clearTracks') {
				labels[i] = 'Will clear';
				if (alt) colors[i] = colEmptyS;
			}
		}
	} else if(trackStates[i] == 1) { // Recording content
		labels[i] = 'Recording';

		if (tracks[i]) {
			colors[i] = colRecS;

			if ((action=='play') || (action=='togglePlayRec')) {
				labels[i] = 'Will play';
				if (alt) colors[i] = colPlayS;
			} else if ((action=='stop') || (action=='mute')|| (action=='toggleStopRec')) {
				labels[i] = 'Will stop';
				if (alt) colors[i] = colStopS;
			}

			if (action=='clearTracks') {
				labels[i] = 'Will clear';
				if (alt) colors[i] = colEmptyS;
			}
		} else {
			colors[i] = colRec;
		}

		if (action=='playRec') {
			labels[i] = 'Will play';
			if (alt) colors[i] = colPlayS;
		}
	} else if(tracks[i]) {
		if ((action == 'mute') || (action == 'stop') || (action == 'play')) {
			colors[i] = RGB(0.5,0.5,0.5);
		} else {
			colors[i] = colEmptyS;
		}
		if (firstEmpty == maxTracks) {
			firstEmpty = i;
			if ((action != 'mute') && (action != 'play') && (action != 'stop'))
				labels[i] = 'Record?';
			if ((action == 'record') || (action == 'toggleAndRecord') || (action == 'playRec')) {
				labels[i] = 'Will record';
				if (alt) colors[i] = colRecS;
			}
		} else {
			if ((action != 'mute') && (action != 'play') && (action != 'stop'))
				labels[i] = 'then record';
		}
	}
	setattribute(VFaders.ary[i],'color',colors[i]);
}

setattribute(Tracks,'colors',colors);
setattribute(Tracks,'labels',labels);
setattribute(MultiPan,'colors',colors);" trigger_script="" trigger_type="4" trigger="1" osc_message="/_updTrks" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="stopRecording()" script="setSelectedTrack(recordingTrack);
recordingTrack = maxTracks;

if (muteRecorded) {
	trackStates[selectedTrack] = 2;
	noteout(0,33,127,1);
	noteout(0,33,0,1);
} else {
	trackStates[selectedTrack] = 3;
	noteout(0,23,127,1);
	noteout(0,23,0,1);
}

if (Mute.x) {
	Mute.reset();
	muteSelExSel();
}" trigger_script="" trigger_type="4" trigger="1" osc_message="/stopRecording" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="muteSelExSel()" script="// Mutes all the tracks except selectedTrack

selectedTracks[selectedTrack] = 0;
toSelectedTracks('mute');
selectedTracks[selectedTrack] = 1;" trigger_script="" trigger_type="4" trigger="1" osc_message="/muteSelExSel" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
