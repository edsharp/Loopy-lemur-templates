	muteRecorded = 0;
	if (Mute.x) {
		setNextAction('toggleAndRecord');
	} else {
		setNextAction('record');
	}
} else {
	if (!Mute.x) {
		// No mute key, so rec -> play
		muteRecorded = 0;
		setNextAction('playRec');
	} else if (muteRecorded) {
		// Mute has been re-pressed, so rec -> play, then toggle all
		setNextAction('toggleStopRec');
	} else {
		// Mute has been held, so toggle all will
		setNextAction('togglePlayRec');
	}
}
